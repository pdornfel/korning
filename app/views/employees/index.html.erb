<h1>Employees</h1>

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <!-- More attributes here... -->
    </tr>
  </thead>

  <tbody class='table'>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.first_name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.email %></td>
        <!-- More attributes here... -->
      </tr>
    <% end %>
  </tbody>
</table>